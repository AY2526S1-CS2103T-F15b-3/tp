@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":ModelManager" as ModelManager MODEL_COLOR
participant ":AddressBook" as AddressBook MODEL_COLOR
participant ":UniquePersonList" as UniquePersonList MODEL_COLOR
end box

[-> ModelManager : hasSamePhoneNumber(toAdd)
activate ModelManager

ModelManager -> AddressBook : hasSamePhoneNumber(toAdd)
activate AddressBook

AddressBook -> UniquePersonList : containsPhoneNumber(toAdd)
activate UniquePersonList

UniquePersonList --> AddressBook : true
deactivate UniquePersonList

AddressBook --> ModelManager : true
deactivate AddressBook

[<-- ModelManager : true
deactivate ModelManager

@enduml
