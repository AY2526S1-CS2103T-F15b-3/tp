@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":TagGroupCommandParser" as TagGroupCommandParser LOGIC_COLOR
participant "tg:TagGroupCommand" as TagGroupCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("tg propertyType")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("tg propertyType")
activate AddressBookParser

create TagGroupCommandParser
AddressBookParser -> TagGroupCommandParser
activate TagGroupCommandParser

TagGroupCommandParser --> AddressBookParser
deactivate TagGroupCommandParser

AddressBookParser -> TagGroupCommandParser : parse("propertyType")
activate TagGroupCommandParser

create TagGroupCommand
TagGroupCommandParser -> TagGroupCommand
activate TagGroupCommand

TagGroupCommand --> TagGroupCommandParser : tg
deactivate TagGroupCommand

TagGroupCommandParser --> AddressBookParser : tg
deactivate TagGroupCommandParser

'Hidden arrow to position the destroy marker below the end of the activation bar.
AddressBookParser -[hidden]-> TagGroupCommandParser
destroy TagGroupCommandParser

AddressBookParser --> LogicManager : tg
deactivate AddressBookParser

LogicManager -> TagGroupCommand : execute(model)
activate TagGroupCommand

TagGroupCommand -> Model : hasTagGroup(tagGroup)
activate Model

Model --> TagGroupCommand : false
deactivate Model

TagGroupCommand -> Model : addTagGroup(tagGroup)
activate Model

Model --> TagGroupCommand
deactivate Model

create CommandResult
TagGroupCommand -> CommandResult
activate CommandResult

CommandResult --> TagGroupCommand
deactivate CommandResult

TagGroupCommand --> LogicManager : result
deactivate TagGroupCommand

[<-- LogicManager
deactivate LogicManager

@enduml
